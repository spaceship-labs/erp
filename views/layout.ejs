  <!--<body>-->
<div id="mainwrapper" class="mainwrapper">
    <%- partial('global/header') -%> 
    <%- partial('global/menu') -%> 

    <div class="rightpanel">
        <ul class="breadcrumbs">
            <li><a href="/home/"><i class="fa fa-home"></i></a> <span class="separator"></span></li>
            <li>Tablero</li>
        </ul>
        
        <div class="pageheader">
            <form action="results.html" method="post" class="searchbar">
                <input type="text" name="keyword" placeholder="Escribe para realizar una busqueda" ng-model='searchInput' onfocus="this.placeholder = ''" onblur="this.placeholder = 'Escribe para realizar una busqueda'"/>
            </form>
            <div class="pageicon"><span class="<%= page.icon || 'fa fa-th-large' %>"></span></div>
            <div class="pagetitle">
                <!-- <h5>All Features Summary</h5> -->
                <h1> <%=page.name || 'Dashboard' %></h1>
                <p class="page-description"><%= page.description || '' %></p>
                <!--<p class="page-description">AQUI PODRAS VISUALIZAR Y ADMINISRAR TODO TU PROCESO DE VENTA</p>-->
            </div>
        </div><!--pageheader-->
        
        <div class="maincontent">
            <div class="maincontentinner">
                <div class="row">
			     <%- body %>
                </div><!--row-->
                
                <div class="footer">
                    <div class="footer-left">
                        <span>&copy; 2014</span>
                    </div>
                    <div class="footer-right">
                        <span>Desarrollo: <a href="http://spaceshiplabs.com/">Spaceshiplabs.com</a></span>
                    </div>
                </div><!--footer-->
                
            </div><!--maincontentinner-->
        </div><!--maincontent-->
        
    </div><!--rightpanel-->
    
</div><!--mainwrapper-->

    <!--TEMPLATES-->
    
    <!--TEMPLATES END-->

